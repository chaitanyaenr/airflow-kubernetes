{
    "benchmarks": [
        {
            "name": "uperf",
            "workload": "network-perf",
            "command": "./smoke_test.sh test_cloud",
            "env": {
                "COMPARE": "false"
            }
        },
        {
            "name": "scale",
            "workload": "scale-perf",
            "command": "./run_scale_fromgit.sh",
            "env": {
                "SCALE": "25"
            }
        },
        {
            "name": "cluster_density",
            "workload": "kube-burner",
            "command": "./run_clusterdensity_test_fromgit.sh",
            "env": {
                "JOB_ITERATIONS":"500",
                "JOB_TIMEOUT":"18000",
                "STEP_SIZE": "30s",
                "QPS":"20",
                "BURST":"20",
                "METRICS_PROFILE":"metrics-aggregated.yaml",
                "LOG_LEVEL":"info",
                "LOG_STREAMING":"true",
                "CLEANUP_WHEN_FINISH": "true",
                "CLEANUP": "true"
            }
        },
        {
            "name": "node_density",
            "workload": "kube-burner",
            "command": "./run_nodedensity_test_fromgit.sh",
            "env": {
                "PODS_PER_NODE":"250",
                "NODE_COUNT":"25",
                "JOB_TIMEOUT": "18000",
                "QPS":"20",
                "BURST":"20",
                "STEP_SIZE": "30s",
                "METRICS_PROFILE":"metrics.yaml",
                "LOG_LEVEL":"info",
                "LOG_STREAMING":"true",
                "CLEANUP_WHEN_FINISH": "true",
                "CLEANUP": "true"
            }
        },
	{
            "name": "upgrades",
            "workload": "upgrade-perf",
            "command": "./run_upgrade_fromgit.sh",
            "env": {
                "LATEST":"true",
                "CHANNEL":"stable-4.7",
                "TIMEOUT": "18000",
                "POLL_INTERVAL":"10"
            }
        }  
    ]
}
